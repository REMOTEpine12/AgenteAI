{
  "name": "agente-ai",
  "version": "1.0.0",
  "description": "Proyecto para hacer un Agente de IA usando el SDK de chatgpt",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist"
  },
  "keywords": [
    "nodejs",
    "express",
    "ai",
    "chatgpt",
    "typescript"
